<%= simple_form_for(@user) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <div class="form-group"><%= f.input :first_name %></div>
    <div class="form-group"><%= f.input :last_name %></div>
    <div class="form-group"><%= f.input :email %></div>
    <div class="form-group"><%= f.input :password %></div>
    <div class="form-group"><%= f.input :rin %></div>
    <div class="form-group"><%= f.input :cell_number %></div>
    <div class="form-group"><%= f.input :home_number %></div>
    <div class="form-group"><%= f.input :rpi_address %></div>
    <div class="form-group"><%= f.input :home_address %></div>
    
  </div>
  <br />
  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
